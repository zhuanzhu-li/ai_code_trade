{
  "name": "quant-trading-frontend",
  "version": "1.0.0",
  "description": "量化交易系统前端 - Vue3 + TypeScript，包含完整的单元测试和覆盖率检查功能",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "type-check": "vue-tsc --noEmit",
    "test": "npx vitest",
    "test:ui": "npx vitest --ui",
    "test:run": "npx vitest run",
    "test:coverage": "npx vitest run --coverage",
    "test:all": "npm run type-check && npm run lint && npm run test:coverage",
    "test:watch": "npx vitest --watch",
    "test:ci": "npx vitest run --reporter=verbose --coverage",
    "clean": "rimraf node_modules package-lock.json dist",
    "reinstall": "npm run clean && npm install",
    "test:win": "scripts\\test.bat",
    "test:unix": "scripts/test.sh",
    "test:cross": "node scripts/test.js",
    "test:full": "node scripts/test.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "element-plus": "^2.8.8",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.6",
    "vue": "^3.5.13",
    "vue-echarts": "^6.6.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.0",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.0",
    "@vue/test-utils": "^2.4.6",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "rimraf": "^6.0.1",
    "sass": "^1.83.0",
    "typescript": "^5.7.3",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^6.0.7",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "glob": "^10.3.10",
    "rimraf": "^6.0.1",
    "inflight": "npm:@npmcli/inflight@^1.0.0",
    "rollup": "^4.28.1"
  },
  "resolutions": {
    "@rollup/rollup-win32-x64-msvc": "^4.28.1"
  }
}
